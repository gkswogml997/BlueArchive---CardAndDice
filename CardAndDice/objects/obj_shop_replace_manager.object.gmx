{
    if !is_undefined(target_list[|i])
    {
        instance_create_v(xx-card_width*i,yy,obj_shop_replace_character_card,target_list[|i])
    }else
    {
        instance_create(xx-card_width*i,yy,obj_shop_empty_character_slot)
    }
}

