draw_set_alpha(life-4)
draw_text_outline(100,350,"카페에서 대기했던 학생 수: "+string(global.amount_of_cafe_card),4,out_color,16)
draw_set_alpha(life-5)
draw_text_outline(100,400,"엔젤24에 방문한 횟수: "+string(global.amount_of_shop_enter),4,out_color,16)
