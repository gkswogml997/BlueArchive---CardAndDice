draw_sprite(spr_loading_bg,0,room_width/2,room_height/2)

//패배 승리 str
if !global.victory_flag
{
    draw_sprite_ext(spr_defeat_str,0,200,75,0.5,0.5,0,c_white,1)
    draw_sprite_ext(spr_particle,0,200,75,8,4,0,c_red,0.5)
}else
