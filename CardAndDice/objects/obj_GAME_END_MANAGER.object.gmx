draw_set_alpha(life-7)

var total_point = global.passing_node*1000 + global.amount_of_kill_card*100
                    -global.amount_of_dead_card*150 + global.amount_of_recruit*50
                    +global.amount_of_cafe_card*25 + global.amount_of_shop_enter*25
                    +global.amount_of_craft_enter*15;
draw_set_font(ft_gyeonggi_sz26_Bold)
draw_text_outline(100,550,"종합점수 : "+string(total_point),4,out_color,16)
draw_set_alpha(1)

life += 0.05

if next_game
{
    draw_sprite_ext(spr_1pixel,0,0,0,1280,720,0,c_black,alpha)
    alpha += 0.01
    if alpha &gt; 2
