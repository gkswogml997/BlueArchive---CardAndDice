draw_set_valign(fa_middle)
draw_set_halign(fa_left)
draw_set_color(c_white)
draw_set_font(ft_gyeonggi_sz20_Bold)
var out_color = make_colour_hsv(146,89,105)

draw_set_alpha(life)
draw_text_outline(100,150,"진행 한 노드 수: "+string(global.passing_node),4,out_color,16)
draw_set_alpha(life-1)
draw_text_outline(100,200,"쓰러뜨린 학생 수: "+string(global.amount_of_kill_card),4,out_color,16)
draw_set_alpha(life-2)
