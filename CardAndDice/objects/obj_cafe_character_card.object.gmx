        draw_sprite_ext(skill_list[|i].skill_icon_spr,0,xx+16,yy+16,0.26,0.26,0,c_white,image_alpha)
        draw_set_font(ft_gyeonggi_sz15_Italic_Light)
        draw_set_halign(fa_center)
        draw_set_valign(fa_middle)
        draw_set_color(c_black)
        draw_text(xx+16+92,yy+16,skill_list[|i].skill_name)
        draw_text_ext(xx+100,yy+80,skill_list[|i].skill_description,24,200)
    }
    
    //선택
    draw_set_halign(fa_center)
    draw_set_valign(fa_middle)
    draw_set_font(ft_Ingame_font)
    draw_set_color(c_black)
    
    var xx = view_xview+view_wview*3/4
    var yy = view_yview+view_hview/2
    draw_sprite(spr_cafe_character_report,0,xx,yy)
    draw_text(xx,yy-120,string(cafe_wating_time)+"시간")
    draw_text(xx,yy-20,reason_for_entering_the_cafe)
    if !instance_exists(obj_UI_button)
    {
        button1 = make_UI_button(xx,yy+128,"선택",spr_UI_button_bg_blue,"선택",id)
        button2 = make_UI_button(xx,yy+210,"취소",spr_UI_button_bg_yellow,"취소",id)
    }
    
    if button_return = "선택" 
    {
        instance_deactivate_object(obj_equipment_card)
        mini_mode = true
        instance_destroy(obj_UI_button)
        instance_destroy(obj_cafe_character_card)
        instance_create_v(0,0,obj_cafe_replace_manager,original_card_id)
        button_return = ""
    }
    if button_return = "취소" 
    {
        instance_deactivate_object(obj_equipment_card)
        mini_mode = true
        instance_destroy(obj_UI_button)
        button_return = ""
