    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if !is_undefined(select_slot) and UI_disable()
{
    //선택
    if notice_box_return = ""
    {
        make_notice_box(target_character.hangul_name+"의 "+select_slot.skill_name+"과 "+target_skill.skill_name+"를 교체하는 것이 확실합니까?",id)
    }
    
    if notice_box_return = "확인"
    {
        target_skill.owner_id.skill_slot = undefined
        target_skill.owner_id.is_used = true
        target_skill.owner_id = target_character
        var target_index = ds_list_find_index(target_character.skill_list,select_slot)
        ds_list_replace(target_character.skill_list,target_index,target_skill)
        instance_destroy(select_slot)
        instance_destroy()
    }
    if notice_box_return = "취소"
    {
        select_slot = undefined
        notice_box_return = ""
    }

}
</string>
          </argument>
        </arguments>
      </action>
