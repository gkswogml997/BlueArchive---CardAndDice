            <kind>1</kind>
            <string>//활성화 여부에 따라 위치
if is_activate
{
    if x &gt; view_xview+view_wview*3/4 
    {
        x -= 60
    }
    y = view_yview+view_hview/2 + 150*(image_index-1) +100
}
else
{
    if x &lt; view_xview+view_wview+sprite_width/2
    {
        x += 60
    }
}
//변경중에는 비활성화
if instance_exists(obj_craft_chamber_replace_manager)
{
    is_activate = false
}else
{
    is_activate = true
}

//스킬 제조
if is_make and !is_used
{
    if is_undefined(skill_slot)
    {
        var gacha_list = ds_map_find_value(obj_GAME_SYSTEM_MANAGER.gacha_table,"skill")
        var rand_num = gacha_list[| irandom(ds_list_size(gacha_list)-1)]
        skill_slot = instance_create_v(0,0,obj_skill_card,rand_num,id)
    }
    if !instance_exists(obj_craft_chamber_replace_manager)
    {
        skill_slot.mini_mode = true
        
    }
}

//스킬슬롯이 비어있지 않고 스킬 교체중일떄 타겟이 아닐 경우 이 오브젝트를 따라간다.
if !is_undefined(skill_slot)
{
    if instance_exists(obj_craft_chamber_replace_manager)
    {
        if obj_craft_chamber_replace_manager.target_skill != skill_slot
        {
            skill_slot.x = x
            skill_slot.y = y
        }
    }else
    {
        skill_slot.x = x
        skill_slot.y = y
    }
}
