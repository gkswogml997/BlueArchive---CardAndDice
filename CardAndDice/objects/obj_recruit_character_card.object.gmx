        <arguments>
          <argument>
            <kind>1</kind>
            <string>sprite_force_scroll+=0.3
if sprite_force_scroll &gt; 30 {sprite_force_scroll = 0}

{
    x = xstart
    y = ystart
    depth = -1021
    draw_sprite(spr_mini_card,0,x,y)
    if obj_recruit_gacha_select.select_card = id
    {
        draw_sprite(spr_my_turn_player_arrow,sprite_force_scroll,x,y-mini_mode_height/2-32)
        draw_sprite_ext(spr_particle,0,x,y,8,8,0,c_aqua,0.8)
        depth = -1022
    }
    draw_sprite(card_portrait,0,x,y)
    draw_sprite(spr_portrait_outline,0,x,y)
}

if mouse_enter()
{
    var xx = view_xview+view_wview/2
    var yy = view_yview+view_hview/2
    
    draw_set_halign(fa_center)
    draw_set_valign(fa_middle)
    
    depth = -1400
    //카드
    draw_sprite(sprite_index,0,xx,yy)
    draw_sprite_ext(card_portrait,0,xx-sprite_width/2+90,yy-sprite_height/2+115,0.35,0.35,0,c_white,image_alpha)
    draw_sprite_ext(spr_portrait_outline,0,xx-sprite_width/2+90,yy-sprite_height/2+1150,0.35,0.35,0,c_white,image_alpha)
    
    
    //이름
    draw_set_color(c_black)
    draw_set_font(ft_eng_name_font)
    draw_text_ext_colour(xx-sprite_width/2+155+155,yy-sprite_height/2+130,name,25,300,c_black,c_black,c_black,c_black,image_alpha/2)
    draw_set_font(ft_name_font)
    draw_text(xx-sprite_width/2+140+155,yy-sprite_height/2+115,hangul_name)
    
    //학교
    draw_sprite_ext(spr_school_icon,school_data,xx,yy-50,image_xscale,image_yscale,image_angle,c_white,image_alpha/4)
    var school_name_str = "정의되지 않은 학교 명"
    switch school_data
    {
        case 1: {school_name_str = "아비도스 고등학교"; break;}
        case 2: {school_name_str = "게헨나 학원" break;}
        case 3: {school_name_str = "밀레니엄 사이언스 스쿨" break;}
        case 4: {school_name_str = "트리니티 종합학원" break;}
        case 5: {school_name_str = "백귀야행 연합학원" break;}
        case 6: {school_name_str = "산해경 고급중학교" break;}
        case 7: {school_name_str = "붉은겨울 연방학원" break;}
        case 8: {school_name_str = "발키리 경찰학교" break;}
        case 9: {school_name_str = "SRT 특수학원" break;}
        case 10: {school_name_str = "아리우스 분교" break;}
        case 11: {school_name_str = "소속불명" break;}
    }
    draw_set_colour(c_gray)
    draw_text(xx,yy-50,school_name_str)
    
    //스텟창
    draw_set_font(ft_Ingame_font)
    draw_set_color(c_white)
    draw_sprite(spr_card_stat,0,xx,yy+sprite_height/4)
    draw_text(xx-75,yy+sprite_height/4-16,"공격력:"+string(attack_power))
    draw_text(xx-75,yy+sprite_height/4+16,"방어력:"+string(defence_power))
    draw_text(xx+75,yy+sprite_height/4-16,"신속:"+string(rapid_power))
    draw_text(xx+75,yy+sprite_height/4+16,"도발:"+string(taunt_power))
    
    var final_skill_level = skill_level
    var skill_level_str = "빈번히 실패함"
    switch final_skill_level
    {
        case 0: {skill_level_str = "빈번히 실패함" break;}
        case 1: {skill_level_str = "가끔 실패함" break;}
        case 2: {skill_level_str = "무난함" break;}
        case 3: {skill_level_str = "능숙함" break;}
        case 4: {skill_level_str = "달인" break;}
    }
    draw_text(xx,yy+sprite_height/4+64,"숙련도:"+skill_level_str)
    
     //스킬창
    for(var i = 0; i &lt; ds_list_size(skill_list); i++)
    {
        var xx = view_xview+view_wview/2 + sprite_width/2
        var yy = view_yview+view_hview/2 - sprite_height/2 + sprite_get_height(spr_large_skill_card)*i
        draw_sprite(spr_large_skill_card,0,xx,yy)
        draw_sprite(skill_list[|i].skill_icon_background,0,xx+16,yy+16)
        draw_sprite_ext(skill_list[|i].skill_icon_spr,0,xx+16,yy+16,0.26,0.26,0,c_white,image_alpha)
        draw_set_font(ft_gyeonggi_sz15_Italic_Light)
        draw_set_halign(fa_center)
        draw_set_valign(fa_middle)
        draw_set_color(c_black)
        draw_text(xx+16+92,yy+16,skill_list[|i].skill_name)
        draw_text_ext(xx+100,yy+80,skill_list[|i].skill_description,24,200)
    }
}


