        p_list[|i].fire_debuff = false
        p_list[|i].good_stamp = false
        p_list[|i].is_stun = false
        p_list[|i].energy_charge = 0
        p_list[|i].dodge_chance = 0
        p_list[|i].is_fear = false
    }
    
    
}

